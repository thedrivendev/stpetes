    "advanced_content_qNdLhQ": {
      "type": "advanced-content",
      "blocks": {
        "image_K7HmEJ": {
          "type": "image",
          "settings": {
            "image": "shopify:\/\/shop_images\/intro_to_tying.jpg",
            "link": "shopify:\/\/blogs\/classes-and-events\/intro-to-fly-tying",
            "width": "50%",
            "alignment": "center"
          }
        },
        "liquid_bzhkg9": {
          "type": "liquid",
          "settings": {
            "code": "<h2>Intro to Fly Tying<\/h2><p>Join us for a tying session at our shop! We would love to introduce you to the next step of your fly-fishing journey. Intro to Fly Tying focuses on the basic tools, materials, and techniques used to tie a variety of different flies.<\/p>",
            "width": "50%",
            "alignment": "center"
          }
        }
      },
      "block_order": [
        "image_K7HmEJ",
