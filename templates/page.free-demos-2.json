    "advanced_content_WGj6TY": {
      "type": "advanced-content",
      "blocks": {
        "liquid_4nWgjP": {
          "type": "liquid",
          "settings": {
            "code": "<h2>On Water Streamer Tactics<\/h2><p>Streamer fishing is one of the most popular ways to catch fish on a fly rod. Like every technique, there is a learning curve to streamer fishing and certain things to understand in order to fish these flies effectively.<\/p>",
            "width": "50%",
            "alignment": "center"
          }
        },
        "image_djyfcC": {
          "type": "image",
          "settings": {
            "image": "shopify:\/\/shop_images\/goldie_in_face.jpg",
