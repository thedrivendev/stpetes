    "advanced_content_BnU4Bi": {
      "type": "advanced-content",
      "blocks": {
        "image_Ky8hQB": {
          "type": "image",
          "settings": {
            "image": "shopify:\/\/shop_images\/soft_hackle_tying.jpg",
            "link": "",
            "width": "50%",
